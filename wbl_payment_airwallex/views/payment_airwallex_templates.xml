<odoo>

    <!--   Redirect Form     -->
    <template id="redirect_form">
        <form t-att-action="api_url" method="post">
            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
            <input type="hidden" name="reference" t-att-value="reference"/>
        </form>
    </template>

    <!--  airwallex Card Page -->
    <template id="inline_form">
         <input type="hidden" name="wbl_success_url" id="wbl_success_url" t-att-value="success_url"/>
         <input type="hidden" name="wbl_fail_url" id="wbl_fail_url" t-att-value="fail_url"/>
        <div id="dropIn"></div>
    </template>
</odoo>
